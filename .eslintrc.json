{
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "env": {
	"browser": true,
	"jquery": true
    },
    "parserOptions": {
	"impliedStrict": true
    },
    "rules": {
	"quotes": ["error", "single"],
	"comma-dangle": ["error", "only-multiline"]
    },
    "globals": {
	"moment": false,
	"Highcharts": false
    }
}
